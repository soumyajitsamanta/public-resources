public class PopUpActions {
   public static final String
       FLATTEN_DIMENSIONS,
       GROUP_HIERARCHIES_BY_DIMENSION,
       SEND_TO_ROWS,
       SEND_TO_COLUMNS,
       SEND_TO_MEASURES,
       SEND_TO_PAGES;

    public static HashMap popUpCaptions;
    static {
       FLATTEN_DIMENSIONS = "0";
       GROUP_HIERARCHIES_BY_DIMENSION = "1";
       SEND_TO_ROWS = "2";
       SEND_TO_COLUMNS = "3";
       SEND_TO_MEASURES = "4";
       SEND_TO_PAGES = "5";


       popUpCaptions = new HashMap(10);
       popUpCaptions.put(FLATTEN_DIMENSIONS,"Flatten dimensions (lose hierarchies)");
       popUpCaptions.put(GROUP_HIERARCHIES_BY_DIMENSION,"Group hierarchies by dimension");
       popUpCaptions.put(SEND_TO_ROWS, "Send to rows");
       popUpCaptions.put(SEND_TO_COLUMNS, "Send to columns");
       popUpCaptions.put(SEND_TO_MEASURES, "Send to measures");
       popUpCaptions.put(SEND_TO_PAGES, "Send to pages");
    }

}@


