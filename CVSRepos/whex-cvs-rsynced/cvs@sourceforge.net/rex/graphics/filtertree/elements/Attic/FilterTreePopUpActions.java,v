public class FilterTreePopUpActions {
   public static final String
       ENABLE_ALL_BUT_THIS_ELEMENT,
       ENABLE_ONLY_THIS_ELEMENT,
       ENABLE_THIS_ELEMENT,
       DISABLE_THIS_ELEMENT,
       LOSE_FILTER,
       APPLY_FILTER,
       MOVE_TO_TAB,
       MOVE_TO_SPLIT_PANE;

    public static HashMap popUpCaptions;
    static {
       ENABLE_ALL_BUT_THIS_ELEMENT = "0";
       ENABLE_ONLY_THIS_ELEMENT = "1";
       ENABLE_THIS_ELEMENT = "2";
       DISABLE_THIS_ELEMENT = "3";
       LOSE_FILTER = "4";
       APPLY_FILTER = "5";
       MOVE_TO_TAB = "6";
       MOVE_TO_SPLIT_PANE = "7";


       popUpCaptions = new HashMap(10);
       popUpCaptions.put(ENABLE_ALL_BUT_THIS_ELEMENT,"Enable all elements but this");
       popUpCaptions.put(ENABLE_ONLY_THIS_ELEMENT,"Enable ONLY this element");
       popUpCaptions.put(ENABLE_THIS_ELEMENT, "Enable element");
       popUpCaptions.put(DISABLE_THIS_ELEMENT, "Disable element");
       popUpCaptions.put(LOSE_FILTER, "Lose filter");
       popUpCaptions.put(APPLY_FILTER, "Apply filter to query(refresh query)");
       popUpCaptions.put(MOVE_TO_TAB, "Display filter in tab");
       popUpCaptions.put(MOVE_TO_SPLIT_PANE, "Display filter in split-pane");

    }

}
