package rex.olap.model;


/**
 * Property of a Member
 * @@author av
 */
public interface Property extends Visitable, Decorator, Displayable, PropertyHolder, Alignable {
  /**
   * @@return the name of the Property
   */
  String getName();
  
  /**
   * the value of the property
   */
  String getValue();
  
}
