public class TupleMemberLabelPopUpActions {

   public static final String
       MOVE_DIMENSION_UP,
       MOVE_DIMENSION_FIRST,
       MOVE_DIMENSION_LAST,
       REMOVE_DIMENSION_FROM_QUERY,
       REMOVE_MEMBER_FROM_QUERY,
       KEEP_THIS_MEMBER_ONLY,
       SEND_MEMBER_TO_FILTER,
       SORT_BY_THIS_ASCENDING,
       SORT_BY_THIS_DESCENDING,
       SORT_BY_THIS_BASCENDING,
       SORT_BY_THIS_BDESCENDING;

    public static HashMap popUpCaptions;
    static {
       MOVE_DIMENSION_UP           = "0";
       MOVE_DIMENSION_FIRST        = "1";
       MOVE_DIMENSION_LAST         = "2";
       REMOVE_DIMENSION_FROM_QUERY = "3";
       REMOVE_MEMBER_FROM_QUERY    = "4";
       KEEP_THIS_MEMBER_ONLY       = "5";
       SEND_MEMBER_TO_FILTER       = "6";
       SORT_BY_THIS_ASCENDING      = "7";
       SORT_BY_THIS_DESCENDING     = "8";
       SORT_BY_THIS_BASCENDING     = "9";
       SORT_BY_THIS_BDESCENDING    = "10";


       popUpCaptions = new HashMap(10);
       popUpCaptions.put(MOVE_DIMENSION_UP          , "Move dimension up");
       popUpCaptions.put(MOVE_DIMENSION_FIRST       , "Move dimension first");
       popUpCaptions.put(MOVE_DIMENSION_LAST        , "Move dimension last");
       popUpCaptions.put(REMOVE_DIMENSION_FROM_QUERY, "Remove dimension from query");
       popUpCaptions.put(REMOVE_MEMBER_FROM_QUERY   , "Remove this member from query");
       popUpCaptions.put(KEEP_THIS_MEMBER_ONLY      , "Keep this member only");
       popUpCaptions.put(SEND_MEMBER_TO_FILTER      , "Send this member to filter");
       popUpCaptions.put(SORT_BY_THIS_ASCENDING     , "Sort by this member(dim.) ASCending (keep hier.)");
       popUpCaptions.put(SORT_BY_THIS_DESCENDING    , "Sort by this member(dim.) DESCending (keep hier.)");
       popUpCaptions.put(SORT_BY_THIS_BASCENDING    , "Sort by this member(dim.) ASCnding (Break.hier.)");
       popUpCaptions.put(SORT_BY_THIS_BDESCENDING   , "Sort by this member(dim.) DESCending (Break.hier.)");


    }

}
