text
@DROP DATABASE IF EXISTS bee;
CREATE DATABASE bee;

DROP TABLE IF EXISTS bee.shell;
CREATE TABLE bee.shell (
	bin		varchar(32) default '',
	name	varchar(64) default ''
);

INSERT INTO bee.shell VALUES ("/bin/sh","shell");
INSERT INTO bee.shell VALUES ("/bin/bash","Bourne Again Shell");
INSERT INTO bee.shell VALUES ("/bin/zsh","Z Shell");
INSERT INTO bee.shell VALUES ("/bin/ksh","Korn Shell");
INSERT INTO bee.shell VALUES ("/bin/csh","C Shell");

/* we don't have to CREATE TABLE who, it's created by task.scheme */

GRANT ALL PRIVILEGES ON bee   TO bee IDENTIFIED BY 'honey';
GRANT ALL PRIVILEGES ON bee.* TO bee IDENTIFIED BY 'honey';
FLUSH PRIVILEGES;

@


