DROP TABLE IF EXISTS lu_state;
CREATE TABLE lu_state (
	id		int NOT NULL AUTO_INCREMENT;
	name	char(5),
	PRIMARY KEY (id),
);

DROP TABLE IF EXISTS lu_domain;
CREATE TABLE lu_domain (
	id			int NOT NULL AUTO_INCREMENT,
	state_id	int,
	name		char(255),
	PRIMARY KEY (id),
#	UNIQUE KEY (name)
);
# CREATE INDEX id_domain ON lu_domain(id);

DROP TABLE IF EXISTS lu_hostname;
CREATE TABLE lu_state (
	id			int NOT NULL AUTO_INCREMENT;
	domain_id	int,
	name		char(255),
	PRIMARY KEY (id),
);


DROP TABLE IF EXISTS fact;
CREATE TABLE fact (
	hostname_id	int,
);
