<input name="Array2"  type='stream' />

<output name="Output" type='stream' />

<code>
<![CDATA[

while(1) {
	my @@array1 = $Array1->fetch;
	my @@array2 = $Array2->fetch;
	$Output->put(@@array1, @@array2);
}

