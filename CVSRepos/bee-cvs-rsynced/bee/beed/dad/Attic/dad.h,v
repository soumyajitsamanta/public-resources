#ifndef DAD_INCLUDED
#define DAD_INCLUDED

#include<netinet/in.h>
#include<stdlib.h>
#include<stdio.h>

#ifndef bool
typedef unsigned char bool;
#endif
#ifndef true
#define true  (1)
#define false (0)
#endif

// zatim
typedef unsigned int Net;

#define error(fmt,...) { \
	fprintf(stderr,"error: %s\n",fmt);	\
	exit(1);							\
}

struct bee_t;

typedef struct client_t {
	Net net;
	struct bee_t *bee;
	// queue => [], # pending messages to client

	struct client_t *next;
} client_t;

typedef struct bee_t {
	char* name;
	int pid;
	int in,out,err; // fds

	struct client_t *client;

	struct bee_t *next;
} bee_t;


#endif /* DAD_INCLUDED */
