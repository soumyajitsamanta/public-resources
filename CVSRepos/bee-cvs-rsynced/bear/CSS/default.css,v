.ttr{border-top:2px solid rgb(102,102,102); border-right:1px solid rgb(102,102,102); padding: 0px; margin: 0px} /*horizontal and vertical border bold*/
.td{border-top:1px solid rgb(102,102,102); border-right:1px dashed rgb(102,102,102); padding: 0px; margin: 0px} /*horizontal and vertical border thin-dashed*/
.tr{border-top:1px solid rgb(102,102,102); border-right:1px solid rgb(102,102,102); padding: 0px; margin: 0px} /*horizontal and vertical border thin*/

.r {border-right:1px solid 102,102,102; padding: 1px; margin: 0px} /*vertical border bold*/
.d {border-right:1px solid 102,102,102; padding: 1px; margin: 0px} /*vertical border thin-dashed*/

/*C O L O R S*/
.ee{background-color: rgb(112,204,112);  text-align:right; font-size: 10px; font-family: serif; color: rgb(0,25,25); padding: 1px;} /*color even even (first dark) */
.eo{background-color: rgb(160,229,160);  text-align:right; font-size: 10px; font-family: serif; color: rgb(0,25,25); padding: 1px;} /*color even odd (first light)*/
.oe{background-color: rgb(216,216,75);  text-align:right; font-size: 10px; font-family: serif; color: rgb(0,25,25); padding: 1px;} /*color odd even (second dark)*/
.oo{background-color: rgb(242,242,123);  text-align:right; font-size: 10px; font-family: serif; color: rgb(0,25,25); padding: 1px;} /*color odd odd (second light)*/
.set {background-color: rgb(255,0,0) !important;  color: rgb(0,25,25);}

/*color row header */
TH.ee{background-color: rgb(112,204,112);  text-align:center; font-size: 10px; font-family: serif; color: rgb(0,25,25);} /*color even even (first dark) */
TH.eo{background-color: rgb(160,229,160);  text-align:center; font-size: 10px; font-family: serif; color: rgb(0,25,25);} /*color even odd (first light)*/
TH.oe{background-color: rgb(216,216,75);  text-align:center; font-size: 10px; font-family: serif; color: rgb(0,25,25);} /*color odd even (second dark)*/
TH.oo{background-color: rgb(242,242,123);  text-align:center; font-size: 10px; font-family: serif; color: rgb(0,25,25);} /*color odd odd (second light)*/

TH.r {border-top:1px dotted rgb(102,102,102); border-right:1px solid rgb(102,102,102); padding: 1px; margin: 0px} /*vertical border bold*/
TH.d {border-top:1px dotted rgb(102,102,102); border-right:1px dashed rgb(102,102,102); padding: 1px; margin: 0px} /*vertical border thin-dashed*/

TH.ch{background-color: rgb(0,0,125); text-align: center; font-size: 10px; font-family: serif; color: rgb(204,204,204); padding: 1px; margin: 0px;}  /*color row header */ 
TH.dv{border-right: 1px dotted rgb(102,102,102);}

/*border column header */
