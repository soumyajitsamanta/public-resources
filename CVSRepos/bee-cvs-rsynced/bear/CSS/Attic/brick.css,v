@/* this part does not affect the REPORT, is used to proper view of style in format page on both Mozilla and IE based web browsers*/
BODY.graph {background-color:transparent; padding: 0; margin: 0px, 0px, 0px, 5px;}
TABLE.graph {border-style: none; empty-cells:show; padding:0; border-spacing:0;}
TABLE.graph  TD { color: inherit; background-color: inherit; text-align: center;}
SPAN.cursor {white-space:nowrap; font-weight:normal; cursor:pointer; cursor:hand;}
TABLE.Neutral {	color: inherit;
		background-color: inherit;
		border: none;
		text-align:left;
		margin:0px; }
TD.Neutral {text-align:left; font-weight:bold; padding:5px;}
TD.bcg { background-color: #eee; }

THEAD.t{ background-color: #ffb056; border-bottom: 2px solid #b25d03; padding: inherit; margin: inherit;}	/*color even even (first dark) */
TBODY.t1 {background-color: #ffcf82; border-bottom: 2px solid #b25d03; padding: inherit; margin: inherit;} /*color odd even (second dark)*/
TR.t1 {background-color: #ffdda8;}									/*color odd odd (second light)*/
TBODY.t2 {background-color: #ffb056; border-bottom: 2px solid #b25d03; padding: inherit; margin: inherit;} /*color even even (second dark) */
TR.t2 {background-color: #ffc482}									/*color even odd (first light)*/

TD.None { color: inherit;  background-color: inherit;  border: none;}

/* this part defines fonts, colors and borders of the REPORT*/

/*T A B L E  L A Y O U T*/
BODY {background-color:#eee}
TABLE {	font-family: Arial, sans-serif;
	font-size: 14px;
	color: #000000;
	border: 4px solid #b25d03;
	border-collapse: collapse;
	padding: 1px;
	margin: 5px;
	white-space: nowrap;
	empty-cells: show;}

TH {padding: 1px; margin: 0; text-align: center !important; white-space: nowrap;}
TR {padding: 1px; margin: 0; white-space: nowrap;}
TD {padding: 1px; margin: 0; text-align: right; white-space: nowrap;}

/*B O R D E R S*/
.t { border-top: 1px solid #b25d03; padding: 1px; margin: 0;}	   /*top*/
.r { border-right: 2px solid #b25d03; padding: 1px; margin: 0;} 			   /*vertical border bold*/
.d { border-right: 1px dashed #b25d03; padding: 1px; margin: 0;}			   /*vertical border thin-dashed*/
.tt{ border-top: 2px solid #b25d03; padding: 1px; margin: 0;}				   /*horizontal border bold */
.tr{ border-top: 1px solid #b25d03; border-right: 2px solid #b25d03; padding: 0; margin: 0;}  /*horizontal and vertical border thin*/
.td{ border-top: 1px solid #b25d03; border-right: 1px dashed #b25d03; padding: 0; margin: 0;} /*horizontal and vertical border thin-dashed*/
.ttr{ border-top: 2px solid #b25d03; border-right: 2px solid #b25d03; padding: 0; margin: 0;} /*horizontal and vertical border bold*/
.ttd{ border-top: 2px solid #b25d03; border-right: 1px dashed #b25d03; padding: 0; margin: 0;}/*horizontal and vertical border bold-dashed*/

/*C O L O R S*/
.ee{ background-color: #ffb056; padding: 1px; margin: 0; text-align:right;}	  	/*color even even (first dark) */
.eo{ background-color: #ffc482; padding: 1px; margin: 0; text-align:right;}	  	/*color even odd (first light)*/
.oe{ background-color: #ffcf82; padding: 1px; margin: 0; text-align:right;}	  	/*color odd even (second dark)*/
