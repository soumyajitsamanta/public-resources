@#!/usr/bin/perl -w

use IO::Handle;
use Socket;
use Data::Dumper;
use Storable qw(freeze thaw nstore_fd retrieve_fd);

my $socket =  "../tmp/cached.sock";

socket(SOCK,PF_UNIX,SOCK_STREAM,0) or die $!;
connect(SOCK,sockaddr_un($socket)) or die $!;
binmode(SOCK,":unix");

nstore_fd \@@ARGV, \*SOCK;
#IO::Handle::flush(SOCK); # important?

my $thaw = eval { retrieve_fd(\*SOCK) };
print Dumper($thaw);

@
