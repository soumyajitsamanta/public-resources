@#include<stdio.h>
#include<signal.h>

void handler(int s) {
	printf("signal %d received\n",s);
}

int main() {
	int value;
	signal(SIGALRM,handler);
	printf("number: ");
	scanf("%d",&value);
	return 0;
}
