<format base="xz" graph="Histogram">
  <parameters mindiameter="0" mintransparency="0" maxtransparency="1" maxdiameter="1" />
  <lattice>
    <y fontsize="14" color="blue" step="1" format="line" label="visible" font="luximb.ttf" size="1" />
    <x fontsize="14" color="blue" step="1" format="line" label="visible" font="luximb.ttf" size="1" />
    <z fontsize="14" color="blue" step="1" format="line" label="visible" font="luximb.ttf" size="1" />
  </lattice>
  <default SH="quadrilateral" DI="0.3" TR="0">
    <CLR>1</CLR>
    <CLR>0</CLR>
    <CLR>0</CLR>
  </default>
  <axis name="Z" crowfoot="1" font="luximb.ttf" size="1" fontsize="14" color="green" length="10" format="cylinder" />
  <axis name="X" crowfoot="1" font="luximb.ttf" size="1" fontsize="14" color="green" length="10" format="cylinder" />
