@#-----------------------------------------------------
#  Bear Constan Definition
#-----------------------------------------------------
# $Id: const.def,v 1.70 2003/10/16 14:08:51 jiri_schmid Exp $

use utf8;

our @@export = ('pdf','Portable Data Format (pdf)','xls','Excel Sheet (xls)','txt','Text (txt)');
our @@sort = ('AZ','A-Z','ZA','Z-A');

our $ProjectName='StoreDemo';

our $version = '0.01';
our $copyright = '(C) Insight Strategy, a. s., 2002';
our $made = 'mailto:info@@instrat.cz';	# html header link

our $cookie_path;
( $cookie_path=url() )=~s|^http://[^/]*||; $cookie_path=~s|/?[^/]*$||;
our $cookie_exp=''; #+24h

our $bearHost="localhost";
our $bearPort="3000";
our $bearUser="";
our $bearPasswd="";
our $compress_threshold=10000;	# compression threshold for SOAP connection

our $MetricsIdent='-METRICS-';	# identifier of multiple metrics placement in Grid Edit
our $ObjectAttr={
	"$MetricsIdent"=>{'img'=>"img/tree/metric_group",'nick'=>'A'},
};

our $DefaultLanguage = 'EN';	# default locale

our $My_Path;
($My_Path=$ENV{'SCRIPT_FILENAME'})=~s|^(.*/).*$|$1|;

#language mutations and its physical files placement with specifications used in html headers
our $Languages={
	CZ=>{src=>'lang/cz.utf',enc=>'UTF-8',lang=>'cs-CZ', 'view'=>'Český'},
	EN=>{src=>'lang/us.utf',enc=>'UTF-8',lang=>'en-US', 'view'=>'English'}
};

our @@Languages=sort(keys(%{$Languages})); #this items will be despalyed as options on preferences site

our $SrcTabs={			#only sources for tabs in tabs.cgi, in language files must be same tabs, with relevant attributes
#	H=>{cgi=>'home.shtml'},
#	F=>{cgi=>'fold.cgi'},
	R=>{cgi=>'run.cgi'},
	D=>{cgi=>'debug.cgi'},
	L=>{cgi=>'ldm.cgi'},
	P=>{cgi=>'prefer.cgi'},
	A=>{cgi=>'admin.cgi'},
	E=>{cgi=>'edit.cgi'}
};

our @@SrcTabs=keys(%{$SrcTabs});

our $StyleSheet;
our $descr_r=6;		# rows of descriprion window
our $sqlRows=10;		# rows of editable sql window
if (($q)&&($q->user_agent()=~/MSIE/)) { 
	#$StyleSheet="base_msie.css";
	$StyleSheet="base.css";
	our $descr_w=35;	# width of descriprion window
	our $itemWidth=46;	# width of droped itme input area
	our $itemWide=28;	# width of textarea on admin page
	our $name_w=36;		# width of name input area
	our $nameWide=26;		# width of name input area
	our $sqlCols=79;
#	our $frameHeight=77;
	our $frameHeight=63;
	our $iframeHeight='245px';
 }
else { 	
	$StyleSheet="base.css";
	our $descr_w=32;	# width of descriprion window
	our $name_w=31;		# width of name input area
	our $itemWidth=38;	# width of droped itme input area
	our $itemWide=26;	# width of textarea on admin page
	our $nameWide=23;		# width of name input area
	our $sqlCols=73;
	our $frameHeight=48;
	our $iframeHeight='245px';
	};

our $texture_dir = '/var/www/html/cache_texture_png';	# cache texture is out of web authorization
our $cache3Dreport = '/var/www/html/bear/vrml/wrl';	# path to  cache for 3D reports
our $cache2Dreport = '/var/www/html/bear/html';	# path to  cache for 2D reports
our $cacheReport = 'html';	# path to  cache for Grid reports
our $Format2Dreport = 'png';	# 2D reports graphics format
our $TTFFontPath = '/usr/X11R6/lib/X11/fonts/TTF';	# path to TTF Fonts

our $graph2Ddefinitions = 'const';
our $graph2Dicons = 'img/graph';
our $graph2Dlogoicons = 'img/logo';
our $graph3Dicons = 'img/vrml';
our $graph3Dtypes = 'img/vrmltypes';

our @@predefinedColors = (
	["255,255,255","229,229,229","204,204,204","178,178,178","153,153,153","127,127,127","102,102,102","67,67,67","33,33,33","0,0,0"],
	["255,0,0","229,0,0","204,0,0","178,0,0","153,0,0","127,0,0","102,0,0","76,0,0","51,0,0","25,0,0"],
	["0,255,0","0,229,0","0,204,0","0,178,0","0,153,0","0,127,0","0,102,0","0,76,0","0,51,0","0,25,0",],
	["0,0,255","0,0,229","0,0,204","0,0,178","0,0,153","0,0,127","0,0,102","0,0,76","0,0,51","0,0,25",],
	["255,255,0","229,229,0","204,204,0","178,178,0","153,153,0","127,127,0","102,102,0","76,76,0","51,51,0","25,25,0"],
	["255,0,255","229,0,229","204,0,204","178,0,178","153,0,153","127,0,127","102,0,102","76,0,76","51,0,51","25,0,25",],
