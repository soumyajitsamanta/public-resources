1.9
log
@remove unnecesary test file
@
text
@#!/usr/bin/perl -w

use SOAP::Lite +autodispatch => 
	uri => 'Bear',
	proxy => ['http://bear.instrat.cz:3000/',
	options => {compress_threshold => 10000}]
;

use Data::Dumper;
print ( SOAP->about );

my @@p= SOAP->GetProjects;
print join(',',@@p)."\n";
#print Dumper (SOAP->GetMetadataShortList($p[0],'grid'));
#print Dumper (SOAP->GetMetadata('6b5640d820',$p[0]));
print Dumper (SOAP->GetMetadata('58dc322605'));

__END__

SOAP->GetMetadata();
if (SOAP::Lite->self->call->fault) {
	print "Faultstring: ".SOAP::Lite->self->call->faultstring."\n";
	print "Faultcode: ".SOAP::Lite->self->call->faultcode."\n";
	print "Faultactor: ".SOAP::Lite->self->call->faultactor."\n";
} else {
	print Dumper (SOAP::Lite->self->call->result);
}
@


