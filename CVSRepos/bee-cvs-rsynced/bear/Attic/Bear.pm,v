package Bear;

use strict;

use vars qw($MetaData);

my $version = '0.01';
my $copyright = '(C) Insight Strategy, a. s., 2002';

my $MetaData = { 'meta' => [
	{
		'id' => '1',
		'type' => 'DIR',
		'name' => 'Attributes',
		'desc' => 'Attributes Directory',
		'cont' => [
			{ 
				'id' => '22',
				'type' => 'ATTR',
				'name' => 'Day',
				'desc' => 'Day of Transaction',
				'def' => {}
			},
			{
				'id' => '6',
				'type' => 'ATTR',
				'name' => 'Month',
				'desc' => 'Day of Transaction',
				'def' => {}
			},
			{
				'id' => '4',
				'type' => 'ATTR',
				'name' => 'Product',
				'desc' => 'Product SKU',
				'def' => {}
			},
		],				
	},
	{
		'id' => '3',
		'type' => 'DIR',
		'name' => 'Metrics',
		'desc' => 'Metrics Directory',
		'cont' => [
			{ 
				'id' => '7',
				'type' => 'METR',
				'name' => 'Revenue',
				'desc' => 'Total Revenue',
				'def' => {}
			},
			{
				'id' => '5',
				'type' => 'METR',
				'name' => 'Cost',
				'desc' => 'Total Cost',
				'def' => {}
			},
		],				
	},
]};	

sub about {                    
  return "Bear Server v$version\n$copyright\n";     
}

sub echo { 
	my ($class, $s) = @@_;
	return $s;
}

sub GetMetadata {
	my ($class, $node, @@param) = @@_;
	my $m = $MetaData->{meta};
	return $m;
}

1;

__END__

