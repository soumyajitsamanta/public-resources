@language change affects TABS immediatly
@
text
@d1 4
a4 6
function re_init_lang(){
var hlp = "tabs.cgi?Lang="+document.preferences.language.value+"&Cook=";
hlp += (document.preferences.cookie.checked) ? "on" : "off";
hlp += "&Open="+document.preferences.open_in.value+"&TabAct=P";
top.frames["tabs"].location.href=hlp;
//top.location.href="index.cgi?TabAct=R
