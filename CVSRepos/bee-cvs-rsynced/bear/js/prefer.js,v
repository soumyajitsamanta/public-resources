1.15
log
@rights implemented
@
text
@// $Id: prefer.js,v 1.14 2004/02/12 16:24:27 jiri_schmid Exp $

function hideLoad() {
	document.getElementById("loading").style.display = "none";
	if (saved){
browsertype = (String(navigator.userAgent.toLowerCase()).indexOf('msie') < 0) ? "mozilla" : "msie";
var str = '';
str += '&Lang='+document.preferences.language.value;
str += '&Open='+document.preferences.open_in.value;
str += '&Length='+document.preferences.tablength.value;
str += '&Proj='+document.preferences.project.value;
str += '&Skin='+browsertype+'/'+document.preferences.skin.value;
str += tabs;
top.frames["tabs"].location.href='tabs.cgi?TabAct=P'+str;
	};
}

function showLoad() {document.getElementById("loading").style.display = "block";}

function objectChanged() {document.getElementById("changed").value = 1;}
