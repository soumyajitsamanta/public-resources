@// $Id: modified.js,v 1.2 2003/12/10 12:23:12 jiri_schmid Exp $

function runModifiedResult(eID){
var open_in = document.getElementById('open_in').value;
switch (open_in) {
 case "_blank": {var EID = window.open('run.cgi?item_id='+eID+"&"+params,eID,"menubar,toolbar=no,statusbar=no,scrollbars,resizable");
 		EID.focus();
		break;}
 case "_one": {	var EID = window.open('run.cgi?item_id='+eID+"&"+params,'result',"menubar,toolbar=no,statusbar=no,scrollbars,resizable");
  		EID.focus();
		break;}
 case "_self": {self.location.href="run.cgi?item_id="+eID+"&"+params;
		return }
 break;
 }
}

function remove(EID,item){
if (confirm(local['Remove Derivat']+": "+EID.alt)) {
	self.location.href="modified.cgi?Action=drop&Item="+item+"&"+params;
}
}
