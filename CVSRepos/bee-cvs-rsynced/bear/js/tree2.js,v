function checkEnglishName() {
var EID = parent.frames["drop_iframe"].document.getElementById('langSpec');
if (EID) {
var i;
for(i=0; i<EID.length; i++) {if (EID.options[i].text == 'EN') break;}
if (EID.options[i].value == "")
	{
	var answer;
	do answer = prompt('English name:',""); while (answer == "");
	EID.options[i].selected = true;
	EID.options[i].value = answer;
	EID.selectedIndex = i;
	parent.frames["drop_iframe"].document.getElementsByName("Action")[0].click();
	}
}
