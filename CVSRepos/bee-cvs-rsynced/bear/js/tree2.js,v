1.56
log
@last used tab is active 4 viewing object, when possible
@
text
@//$Id: tree2.js,v 1.55 2004/02/25 16:05:07 jiri_schmid Exp $

var oldItem;
var GID;
var OC = true;

function refreshTree() {
self.location.href = "edit_tree.cgi?Force=refresh"+use_param;
}

function ocAttr(ID) {
var commonObject = document.getElementById("segment"+ID);
if (commonObject) {
	var OC = (commonObject.style.display == 'block') ? 1 : 0;	//1-block is visible; 0 block is hidden
	commonObject.style.display = (OC) ? 'none' : 'block';
	commonObject = document.getElementById("Attr"+ID);
	if (String(commonObject.src).match(/\/join/)) { return };
	if (String(commonObject.src).match(/bottom/)) {
		commonObject.src = (OC) ? icons['plusBottom'] : icons['minusBottom'];
	} else {
		commonObject.src = (OC) ? icons['plus'] : icons['minus'];
	}
}
}


function ocFolder(EID) {
var commonObject = document.getElementById("segment"+EID.id);
if (commonObject) {
	var OC = (commonObject.style.display == 'block') ? 1 : 0;	//1-block is visible; 0 block is hidden
	commonObject.style.display = (OC) ? 'none' : 'block';
	commonObject = document.getElementById("icon"+EID.id);
	if (String(commonObject.src).match(/\/join/)) { return };
	if (String(commonObject.src).match(/bottom/)) {
		commonObject.src = (OC) ? icons['plusBottom'] : commonObject.src = icons['minusBottom'];
	} else {
		commonObject.src = (OC) ? icons['plus'] : icons['minus'];
	}
	if (OC) {
		document.getElementById("folderIcon"+EID.id).src = icons['folder'];
		if (use_cookie == "off") return true;
		setTreeCookie(EID.id,0);
	} else {
		document.getElementById("folderIcon"+EID.id).src = icons['folderOpen'];
		if (use_cookie == "off") return true;
		setTreeCookie(EID.id,1);
	}
}
return true
}

function ocTree(side) {
var EID = document.getElementById('openClose');
var task = (side) ? side : (String(EID.src).match(/left/)) ? 'close' : 'open';
var iframe = parent.document.getElementById('elements_iframe');
var shrt = (iframe.offsetLeft > 801) ? 2 : 3;
var third = Number(String(iframe.style.width).replace(/.$/,""));
iframe = parent.document.getElementById('drop_iframe');
var wdth = parent.document.getElementById('width').value;
var path = (document.getElementById('skin').value) ? "skins/"+document.getElementById('skin').value+"/" : "";
if (task == 'open') {
//	EID.src = path+'img/leftArr.'+document.getElementById('imgType').value;
	EID.src = path+'img/leftArr.png';
	if (iframe.style) {
		parent.document.getElementById('tree_iframe').style.width = wdth + "%";
		iframe.style.left = wdth + "%";
		iframe.style.width = (100-wdth-third) + "%";
		document.getElementById('content').style.display = 'block';
		initOffset(1);
		OC = true;
	}
} else if (task == 'close') {
//	EID.src = path+'img/rightArr.'+document.getElementById('imgType').value;
	EID.src = path+'img/rightArr.png';
	if (iframe.style) {
		parent.document.getElementById('tree_iframe').style.width = shrt + "%";
		iframe.style.left = shrt + "%";
		iframe.style.width = (100-third-shrt) + "%";
		document.getElementById('content').style.display = 'none';
		initOffset(1);
		OC = false;
	}
}
}


//---------------open tree after load-------------------------------

function openHierarchy(GID){
if (!GID) {
	GID = (document.getElementById('active_id').value) ? document.getElementById('active_id').value : (use_cookie == "on") ? get_cookie("gid") : null;
	};
if (GID) {
	var node = findItemById(GID);
	var cycleOpen = true;
	while (cycleOpen && node) {
		if (document.getElementById("view"+node[1])) {
			document.getElementById(node[1]).onclick();
			node = findItemById(node[1]);
		} else {
			cycleOpen = false;
		}
	}
	openCachedNodes();
	jump('',GID);
	}
}

function get_type(item_id){
return (document.getElementById("icon"+item_id)) ? document.getElementById("icon"+item_id).name.match(/[^\|]*$/) : ""
}

//-----------------------------------------------------------------
function deniedQuit(){
var EID = top.frames["body"].document.getElementById("changed");
if (EID)
	if (EID.value == 1) {
		if (confirm(msg_text('treeInfQ'))) {EID.value = 0; return false}
		else {	return true; }
};
EID.value = 0;
return false
}

//-----------------------------------------------------------------

function jump(EID,id) {
if (deniedQuit()) return;
if (id) EID = document.getElementById(id);
if (EID) {
	var str = document.getElementById("icon"+EID.id).name;
	var typ = String(str).match(/[^\|]*$/);
	if (String(typ).match(/D|G|R|A|Af|M|G3|F|Ro|P/)) {
		var tab = get_cookie("tab");
		parent.frames["drop_iframe"].location.replace("edit_obj.cgi?item_id="+EID.id+use_param+"&Last="+tab);
		parent.frames["tree_iframe"].editType=typ;
		show_load();
	}
	useStyle(EID);
	hideMe();
	GID = EID.id;
	document.getElementById('active_id').value = GID;
	set_cookie("gid",GID);
}
}
//-----------------------------------------------------------------
function useStyle(EID) {
if (oldItem) { document.getElementById("view"+oldItem).className = ""; }
document.getElementById("view"+EID.id).className = 'selected';
oldItem = EID.id;
/*if (oldItem) { oldItem.className = ""; }
EID.className = 'selected';
oldItem = EID;*/
}
//-----------------------------------------------------------------
function showMe(EID) {
var ID = EID.id;
if (ID.match(/root$/)) return;
EID = document.getElementById("icon"+ID);
if (dragLocator.name == mg + "|"+mg_type) {alert(msg_text('mg_alert')); return(false);}
var pole_Name = EID.name.split("|");
if (check_show(pole_Name[1]) == 0) {
	if (pole_Name[1] == "Af") {
		var nodeChild = findItemById(ID);
		var nodeParent = findItemById(nodeChild[1]);
		pole_Name[0] = nodeParent[2] + "~"+ nodeChild[2];
	}
	dragLocator.name = pole_Name[0]+"|"+pole_Name[1];
	dragLocator.icon = EID.src;
	var hlp = divID.innerHTML.replace(/src="[^"]*/,"src='"+dragLocator.icon+"'");//replace the src of icon, which has to be viewed
	divID.innerHTML = hlp.replace(/[^>]*$/,String(dragLocator.name).match(/^[^\|]*/));	//replace the name to be viewed
	dragLocator.id = ID;
	dragLocator.typ = pole_Name[1];
	if (divID.style.visibility == "hidden")	{ divID.style.visibility = "visible"; moveimg();}
} else {
	hideMe();
}}
//-----------------------------------------------------------------
function setTreeCookie(tn,nv){ //tree node, node value (0,1) - close,open
if (use_cookie == "off") return;
var cntnt = get_cookie('TreeNodes');
var rgxp = new RegExp("~?"+tn,"g");
if (nv) {	//add node to list
	if (String(cntnt).match(rgxp)) {
		return
	} else {
		if (cntnt == '') {
			cntnt = tn;
		} else {
			cntnt += "~"+tn;
		}
	}
} else {
	if (String(cntnt).match(new RegExp(tn))) {
		cntnt = String(cntnt).replace(rgxp,'');
		if (String(cntnt).match(/^~/)) {
			cntnt = String(cntnt).replace(/^~/,'');
		}
	}
}
document.cookie = "TreeNodes="+cntnt;
}
//-----------------------------------------------------------------
function openCachedNodes() {
if (use_cookie == "off") return;
var nodes = String(get_cookie('TreeNodes')).split(/~/);
for (var i in nodes) {
	var commonObject = document.getElementById("segment"+nodes[i]);
	if (commonObject) {
		if (commonObject.style.display == 'none') {
			document.getElementById(nodes[i]).onclick();
		}
	}
}
}
//-----------------------------------------------------------------
function closeCachedNodes() {
if (use_cookie == "off") return;
var nodes = String(get_cookie('TreeNodes')).split(/~/);
for (var i in nodes) {
	var commonObject = document.getElementById("segment"+nodes[i]);
	if (commonObject) {
		if (commonObject.style.display == 'block') {
			document.getElementById(nodes[i]).onclick();
		}
	}
}
}
//-----------------------------------------------------------------
function BreakRemove(ids) {
var pole_id = String(ids).split(/~/);
var names = new Array();
for (i in pole_id) {
	var node = findItemById(pole_id[i]);
	names.push(node[2]);
}
var aux = top.frames["body"].window.langJS['breaks'];  
alert(aux+names);
}
//-----------------------------------------------------------------@


