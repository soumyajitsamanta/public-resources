function highlight(itemId){
var oldItem = document.getElementById("passive_id");
if (oldItem.value && GID) {	// old item became common background color
	document.getElementById(oldItem.value).style.backgroundColor=document.getElementById("span"+GID).style.backgroundColor;
};
if (itemId) {
	document.getElementById(itemId).style.backgroundColor="#eef94a"; // new item, became yellow
	oldItem.value = itemId;
};
GID=itemId;
}

function jump(a) {    //a = tree item ID, needed beacause of highliting after dbclick 
hide_me();
if (deniedQuit()) return;
if (a == 0) return;
var typ = get_type(a);
if (typ == "") return;
highlight(a);
document.getElementById("active_id").value=a;
GID = a; set_cookie("id");
  if (String(typ).match(/D|G|Fi|R|A|Af|Fa|Fo|M|G3|F3/)) {
	parent.frames["drop_iframe"].location.replace("edit_obj.cgi?item_id="+a+use_param);
	parent.frames["tree_iframe"].editType=typ;
	show_load();
  }
}

