  return "Basic Edge Weight Estimator v$version ($copyright)"  
}

#---------------------------------------

sub weight {	# function to estimate weight of the edge between 
	my $from=shift;	# starting vertex of the edge
	my $to=shift;	# ending vertex of the edge
#printf "%s -> %s\n", $from, $to;
	return 1;	# weight estimation
}

