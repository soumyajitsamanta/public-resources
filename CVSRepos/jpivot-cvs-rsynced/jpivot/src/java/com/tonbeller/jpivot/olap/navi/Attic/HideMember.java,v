package com.tonbeller.jpivot.olap.navi;

import java.util.List;

import com.tonbeller.jpivot.core.Extension;

/**
 *
 * allows to select members to be hidden
 * @@author wawan
 *
 */
public interface HideMember extends Extension {

	public static final String ID = "hideMember";

	/**
	 * @@return the hiddenList
	 */
	public List getHiddenList();

}
