1.1.2.1
log
@* adding feature for hidden member selection
@
text
@a0 20
<?xml version="1.0" encoding="ISO-8859-1"?>
<xsl:stylesheet version="1.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform">

<xsl:template match="@@*|node()">
   <xsl:copy>
      <xsl:apply-templates select="@@*|node()"/>
   </xsl:copy>
</xsl:template>

<xsl:template match="exp[@@function='Hierarchize']">
	<exp class='FunCall' syntacticType='0' function="Union">
		<xsl:apply-templates select="args/FunCall/args"/>
	</exp>
</xsl:template>

<xsl:template match="FunCall[@@function='Hierarchize']">
		<xsl:apply-templates select="args/FunCall"/>
</xsl:template>

</xsl:stylesheet>
@

