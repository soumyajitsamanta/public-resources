<?xml version="1.0" encoding="ISO-8859-1"?>
<xsl:stylesheet version="1.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform">

<xsl:template match="@@*|node()">
   <xsl:copy>
      <xsl:apply-templates select="@@*|node()"/>
   </xsl:copy>
</xsl:template>


<xsl:template match="exp[@@function='VisualTotals']">
	<exp class='FunCall'  syntacticType='5' function='/{/}'>
		<xsl:apply-templates select="args/FunCall/args"/>
	</exp>
</xsl:template>


<xsl:template match="FunCall[@@function='VisualTotals']">
		<xsl:apply-templates select="args/FunCall"/>
</xsl:template>

</xsl:stylesheet>
