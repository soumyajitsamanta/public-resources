package es.aeat.eett.infoRubik.reports;

import java.awt.event.MouseEvent;
import java.util.EventObject;

import javax.swing.JTree;
import javax.swing.tree.DefaultTreeCellEditor;
import javax.swing.tree.DefaultTreeCellRenderer;

/**
 * @@author f00992
 */
class ReportTreeCellEditor extends DefaultTreeCellEditor {

	/**
	 * @@param tree
	 * @@param renderer
	 */
	public ReportTreeCellEditor(JTree tree, DefaultTreeCellRenderer renderer) {
		super(tree, renderer);
	}
	public boolean isCellEditable(EventObject e){
		if (e instanceof MouseEvent) {
			// Solo se permite editar usando el menu
			return false;
		}
		//prepareForEditing();
		//return true;
		return super.isCellEditable(e);
	}

}
