package es.aeat.eett.rubik.basicStat;

import java.awt.Graphics;
import java.awt.print.PageFormat;
import java.awt.print.Printable;
import java.awt.print.PrinterException;

import javax.swing.JScrollPane;

import es.aeat.eett.rubik.core.util.JTableSimplePrint;


/**
 * @@author f00992
 *
 */
public class ScrollTablePrint extends JScrollPane  implements Printable{
	JTableSimplePrint table=null;


	ScrollTablePrint(JTableSimplePrint table) {
		super(table);
		this.table = table;
	}


	/**
	 * @@see java.awt.print.Printable#print(java.awt.Graphics, java.awt.print.PageFormat, int)
	 */
	public int print(Graphics graphics, PageFormat pageFormat, int pageIndex) throws PrinterException {
		return table.print(graphics, pageFormat, pageIndex);
	}
}
@


